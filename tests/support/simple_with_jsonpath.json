{
  "description": "Simple output with features taken from external file using JsonPath expression",
  "inputs": [
    {
      "name": "attributes",
      "kind": "FILE",
      "source": "tests/support/attributes.json"
    }
  ],
  "output": {
    "tool": "jsonatr",
    "version": "$attributes.version",
    "stable": false,
    "features": "$attributes.features[:-1]"
  }
}