{
  "description": "JsonPath queries to the Apalache counterexample. We are interested only in the last counterexample state",
  "input": [
    {
      "name": "ce",
      "kind": "FILE",
      "source": "tests/support/counterexample.json"
    }
  ],
  "output": {
    "last-state": "ce.declarations[-2]",
    "tooManyFaults": "$ce.declarations[-2]..[?(@.binary == '=' && @.left == 'tooManyFaults')].right",
    "height": "$ce.declarations[-2]..[?(@.left == 'height')].right",
    "verdict": "$ce.declarations[-2]..[?(@.left == 'outEvent')].right.record[?(@.left.str == 'verdict')].right",
    "blockchain": "$ce.declarations[-2]..[?(@.left == 'blockchain')].right",
    "last-block": "$ce.declarations[-2]..[?(@.left == 'blockchain')].right[-1]",
    "last-block-lastCommit": "$ce.declarations[-2]..[?(@.left == 'blockchain')].right[-1]..[?(@.left.str == 'lastCommit')].right..str"
  }
}